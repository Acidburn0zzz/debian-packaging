#!/bin/sh
set -e

case "$1" in
    purge)
        deluser --system --quiet gitea || true
        rm -rf /var/log/gitea /etc/gitea
        ;;

    upgrade|remove|failed-upgrade|abort-install|abort-upgrade|disappear)
        ;;

    *)
        echo "postrm called with unknown argument \`$1'" >&2
        exit 1
        ;;
esac

#DEBHELPER#

exit 0
